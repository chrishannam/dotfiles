export proj=$r/proj
export dt=$r/dt
export MAVEN=$dt/apache-maven
export SCALA_HOME=$dt/scala
export PATH=$HOME/.pyenv/bin:$HOME/.pyenv/shims:/usr/local/sbin:/usr/local/bin:$PATH:$MAVEN/bin:$SCALA_HOME/bin/:~/bin

MANPATH="/usr/local/opt/coreutils/libexec/gnuman:$MANPATH"

# Extend PATH to find `go` binaries
export GOPATH=~/go/

export PIP_DOWNLOAD_CACHE=$HOME/Library/Caches/pip-downloads

export PROJECT_HOME=~/proj/py/
